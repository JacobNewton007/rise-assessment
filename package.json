{
  "name": "rise_test",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "nodemon --exec ts-node ./src/index.ts --mode development",
    "build:digitalocean": "npm install && npm run build",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "pretest": "yarn run lint && cross-env NODE_ENV=test yarn run migrate-test:reset && yarn run migrate-test:up",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text --reporter=text-summary mocha --require ts-node/register --exit --recursive --timeout=150000 tests/**/*.ts",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "posttest": "yarn run migrate-test:down",
    "test:e2e": "jest --config ./tests/jest-e2e.json",
    "migrations": "source .env && db-migrate",
    "migrate:up": "db-migrate up  --config database.json",
    "migrate-test:up": "db-migrate up --config database.json -e test",
    "migrate-test:down": "db-migrate down -c 100 --config database.json -e test",
    "migrate:reset": "db-migrate reset --config database.json",
    "migrate-test:reset": "db-migrate reset --config database.json -e test",
    "seed:create": "db-migrate create:seed",
    "seed:up": "db-migrate up:seed",
    "seed:down": "db-migrate down:seed",
    "seed:reset": "db-migrate reset:seed",
    "seed:reset:test": "db-migrate reset:seed --config database.json -e test",
    "seed:up:test": "db-migrate up:seed --config database.json -e test"
  },
  "dependencies": {
    "@bitreel/sql-quest": "^0.0.6",
    "@types/bcrypt": "^5.0.0",
    "@types/sinon": "^10.0.16",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "bcrypt": "^5.1.1",
    "chai-http": "^4.4.0",
    "connect-redis": "^7.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.3.0",
    "deasyncify": "^0.0.4",
    "device-detector-js": "^3.0.3",
    "dotenv": "^16.3.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "file-stream-rotator": "^1.0.0",
    "http-status-codes": "^2.2.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "lodash.isequal": "^4.5.0",
    "mocha": "^10.2.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "redis": "^4.6.7",
    "sinon": "^15.2.0",
    "superagent": "^8.0.9",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
    "winston-papertrail": "^1.0.5",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@babel/register": "^7.22.5",
    "@types/chai": "^4.3.5",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash.isequal": "^4.5.6",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.3.1",
    "@types/sinon": "^10.0.16",
    "@types/swagger-jsdoc": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "chai": "^4.3.7",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.0",
    "nodemon": "^2.0.22",
    "nyc": "^15.1.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "*"
  }
}
